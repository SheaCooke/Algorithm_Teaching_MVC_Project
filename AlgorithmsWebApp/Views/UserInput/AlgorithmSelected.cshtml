@model AlgorithmModels;

<h1>AlgorithmSelected</h1>

<p>@Model.SelectedAlgorithm</p>

@if (@Model.SelectedAlgorithm.Equals(AlgorithmSelection.BinarySearch))
{
    <form method="post" asp-action="GenerateResponse" asp-controller="UserInput">

          <label for="valueToFind">Value To Find:</label>
          <input type="text" id="valueToFind"><br><br>
          <label for="collectionOfValues">Collection Of Values</label>
          <input type="text" id="collectionOfValues"><br><br>

          <input type="submit" value="Run" />
    </form>
}
else
{
    <p1>Not working</p1>
}